_G.Thing = "Sentry"
_G.Cooldowns = 5.1
local cooldown = false
UIS.InputBegan:Connect(function(Input)
if Input.KeyCode == Enum.KeyCode.F then
if cooldown == true then return end
cooldown = true
game:GetService("ReplicatedStorage")[_G.Thing]:FireServer(true)
     wait(_G.Cooldowns)
cooldown = false
        end
    end)
    end
})

