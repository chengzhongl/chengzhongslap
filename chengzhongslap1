    Tab:AddButton({
	Name = "开球时重新加入按钮",
	Callback = function()
	--Auto execute highly recommended (use it)
gui = game.CoreGui.RobloxPromptGui.promptOverlay:WaitForChild("ErrorPrompt")

gui.Size = UDim2.new(0, 400, 0, 230)

leave = gui.MessageArea.ErrorFrame.ButtonArea.LeaveButton

